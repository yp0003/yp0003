<script language="javascript">
$(function(){
	//$(".cls_course").attr("id","cls_course");
		
	var aPage = $('.slider_num a');		//分页按钮
	var aImg = $('.slider_list li');		//图像集合
	var iSize = aImg.size();		//图像个数
	var index = 0;		//切换索引
	var t;
	$('#btnLeft').click(function(){		//左边按钮点击
		index--;
		if(index<0){
			index=iSize-1;
		}
		change(index);
	})
	$('#btnRight').click(function(){    //右边按钮点击
		index++;
		if(index>iSize-1){
			index=0
		}
		change(index);
	})
	//分页按钮点击
	aPage.click(function(){
		index = $(this).index();
		change(index)
	});
	//切换过程
	function change(index){
		aPage.removeClass('active');
		aPage.eq(index).addClass('active');
		aImg.stop();
		//隐藏除了当前元素，所以图像
		aImg.eq(index).siblings().animate({
			opacity:0
		},1000,function(){aImg.eq(index).siblings().css("display","none")})
		//显示当前图像
		aImg.eq(index).css("display","block");
		aImg.eq(index).animate({
			opacity:1
		},1000)
	}
	 
	function autoshow() {
		index=index+1;
		if(index<=iSize-1){
		   change(index);
		}else{
			index=0;
			change(index);
		}
			
	}
	int=setInterval(autoshow,5000);
	function clearInt() {
		$('#btnLeft,#btnRight,.slider_num a').mouseover(function() {
			clearInterval(int);
		})
	
	}
	function setInt() {
		$('#btnLeft,#btnRight,.slider_num a').mouseout(function() {
			int=setInterval(autoshow,5000);
		})
	}
	
	$(".slider").hover(function(){
		$("#btnLeft,#btnRight").css("visibility","visible");	
	},function(){
		$("#btnLeft,#btnRight").css("visibility","hidden");
	})
	clearInt();
	setInt();
});
</script>
