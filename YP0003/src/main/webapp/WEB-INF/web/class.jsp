<script type="text/javascript">
//<![CDATA[
var cmt_empty_username = "请输入您的用户名称";
var cmt_empty_email = "请输入您的电子邮件地址";
var cmt_error_email = "电子邮件地址格式不正确";
var cmt_empty_content = "您没有输入评论的内容";
var captcha_not_null = "验证码不能为空!";
var cmt_invalid_comments = "无效的评论内容!";

/**
 * 提交评论信息
*/

$(function(){
	$(".comm_btngo").click(function(){
		var comm = {};
		comm.email = $("input[name=email]").val();
		comm.content = $("textarea[name=content]").val();
		comm.type = $("input[name=cmt_type]").val();
		comm.id = $("input[name=id]").val();
		comm.enabled_captcha = $("input[name=enabled_captcha]").val()?$("input[name=enabled_captcha]").val():0;
		comm.captcha = $("input[name=captcha]").val();
		if(comm.type==0){
			comm.rank = $('input:radio[name="comment_rank"]:checked').val();
		}else{
			comm.rank=5;	
		}
		var myreg = /^([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+@([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+\.[a-zA-Z]{2,3}$/;
		if(!myreg.test(comm.email)){
			BF.msg({
				startCode:300,
				title:'评论出错',	
				content:cmt_error_email
			});
			return false;
		}
		if(comm.content.length<1){
			BF.msg({
				startCode:300,
				title:'评论出错',	
				content:cmt_invalid_comments
			});
			
										
										
			return false;
		}
		if (comm.enabled_captcha > 0 && comm.captcha.length == 0 ){
			BF.msg({
				startCode:300,
				title:'评论出错',	
				content:captcha_not_null
			});
		  return false;
	   }
	   if(!/^([a-z]|[A-Z]|\d){4}$/.test(comm.captcha)){
		   BF.msg({
				startCode:300,
				title:'评论出错',	
				content:"验证码输入错误!"   
		   });
		   return false;
	   }
	   
	   //验证内容是否合法
	   $.ajax({
				type: 'POST',
				url:'/checkkwd.php',
				data:{'content':comm.content,'act':'jquery'},
				dataType:"json",
				cache: false,
				success: function(res){
					if(res.start==300){
						BF.msg({
							startCode:res.start,
							content:res.content
						});
						return false;		
					}else{
						$.ajax({
							url:'comment.php',
							data:{cmt:BF.obj2str(comm)},
							type:'post',
							dataType:"json",
							success:function(result){
								if (result.message){
									BF.msg({startCode:200,content:result.message});
								}
								if (result.error == 0){
									$("#ECS_COMMENT").html(result.content);
								}	
							},
							error:function(){
								alert('');
							}	
						});	
					}
				},
				error: function(){
					BF.msg({
						startCode:300,
						content:"系统错误，请稍后再试!"
					});
				}
			});
			
			
   		//Ajax.call('comment.php', 'cmt=' + cmt.toJSONString(), commentResponse, 'POST', 'JSON');
		
		return false;
	});	
})



//]]>
</script></div>
